{
  "name": "node-sidh",
  "version": "0.6.2",
  "description": "Nodejs bindings to SIDH v3.3",
  "main": "lib/index.js",
  "gypfile": true,
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "watch": "tsc -w",
    "copy-nodes": "cp ./build/Release/node-sike.node ./lib/node-sike.node && cp ./build/Release/node-sidh.node ./lib/node-sidh.node && cp ./include/node-sike.d.ts ./lib/ && cp ./include/node-sidh.d.ts ./lib/",
    "rebuild": "node-gyp rebuild && npm run copy-nodes",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aellison5505/node-sidh.git"
  },
  "keywords": [
    "sidh",
    "nodejs"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.1",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "tsdoc": "0.0.4",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^3.0.0"
  },
  "author": "Anthony Ellison",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aellison5505/node-sidh/issues"
  },
  "homepage": "https://github.com/aellison5505/node-sidh#readme"
}
